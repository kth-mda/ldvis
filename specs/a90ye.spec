{
  "title": "nobel laurates",
  "spec": "server\n  http://dbpedia.org/sparql\nquery\n  PREFIX : <http://dbpedia.org/resource/>\n  PREFIX dbo: <http://dbpedia.org/ontology/>\n  PREFIX dbc: <http://dbpedia.org/resource/Category:>\n  PREFIX dcterms: <http://purl.org/dc/terms/>\n  PREFIX dbp: <http://dbpedia.org/property/>\n\n  SELECT ?nobel, ?name, ?univ, ?univName\n  WHERE {\n    ?nobel dcterms:subject dbc:Nobel_laureates_in_Physics .\n    ?nobel dbp:name ?name .\n    ?nobel dbo:almaMater ?univ.\n    ?univ dbp:name ?univName.\n    ?univ dbo:country dbr:Netherlands.\n    FILTER (lang(?name) = \"en\") .\n  }\n\nmapto\n  node('parent').layout('xy');\n  node(?univ).parent('parent').color('yellow').label(?univName).click(?univ);\n  node(?nobel).parent('parent');\n  line(?nobel, 'at', ?univ);\nend\n"
}